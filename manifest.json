{
    "manifest_version": 2,
    "name": "znet",
    "version": "0.1",
    "description": "The Zeal Decentralized Network",
    "icons": {
        "16": "bosnet16.png",
        "48": "bosnet48.png",
        "128": "bosnet128.png"
    },
    "background": {
        "scripts": [
            "znet.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "proxy",
        "*://*.zealchain.com/*",
        "*://*.z/*",
        "*://*.ze/*",
        "*://*.zeal/*",
        "*://*.bit/*",
        "*://*.eth/*",
        "*://*.lib/*",
        "*://*.coin/*",
    ],
    "homepage_url": "http://zealchain.com/"
}