{
    "manifest_version": 2,
    "name": "znet",
    "version": "0.6",
    "description": "The Zealchain Network",
    "icons": {
        "16": "bosnet16.png",
        "48": "bosnet48.png",
        "128": "bosnet128.png"
    },
    "background": {
        "scripts": [
            "znet.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "proxy",
        "*://*.zealchain.com/*",
        "*://*.z/*",
        "*://*.ze/*",
        "*://*.zeal/*",
        "*://*.zn/*",
        "*://*.znet/*",
        "*://*.ze/*",
        "*://*.o/*",

    ],
    "homepage_url": "https://zealchain.com/"
}